Config { font = "xft:FiraCode-Regular:size=10"
       , additionalFonts = [ "xft:FontAwesome:size=11" ]
       , bgColor = "black"
       , fgColor = "white"
       , position = BottomSize C 100 25
       , commands = [ Run UnsafeStdinReader

                    , Run BatteryP ["BAT0"] ["-t", "<timeleft> ~ <left> <acstatus>", "-S", "True",
                                             "-L", "15", "-H", "95",
                                             "--low", "red", "--high", "green",
                                             "--", "-O", "<fn=1></fn>", "-o", "<fn=1></fn>"] 600

                    , Run Network "wlp2s0" ["-t", "<rx><fn=1>   </fn><tx><fn=1>  </fn>", "-S", "True", 
                                            "-L","0","-H","1000000", 
                                            "--low", "white", "--high","red"] 50

                    , Run Com "/home/ronan/.config/scripts/cpu" [] "c" 50

                    , Run Com "/home/ronan/.config/scripts/trayer_padding" [] "trayerpad" 100

                    , Run Com "/home/ronan/.config/scripts/temp" [] "t" 500
                    
                    , Run Com "/home/ronan/.config/scripts/disk" [] "d" 1000

                    , Run Date "%a %b %_d - %H:%M" "date" 600

                    , Run Com "/home/ronan/.config/scripts/brightness" [] "b" 10

                    , Run Com "/home/ronan/.config/scripts/volume" [] "v" 10
                    ]
       , sepChar = "$"
       , template = "$UnsafeStdinReader$ } $date$ { $wlp2s0$ | $d$ <fn=1></fn> | $c$% <fn=1></fn> | $t$°C <fn=1></fn> | $v$<fn=1>   </fn> | $b$<fn=1></fn> | $battery$ |$trayerpad$"
       }
