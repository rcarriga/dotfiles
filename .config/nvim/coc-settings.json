{
    "languageserver": {
        "kotlin": {
            "command": "~/kotlin-language-server/server/build/scripts/kotlin-language-server",
            "filetypes": [
                "kotlin"
            ]
        },
        "clangd": {
            "command": "clangd",
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "filetypes": [
                "c",
                "cpp",
                "objc",
                "objcpp"
            ]
        },
        "haskell": {
            "command": "hie-wrapper",
            "rootPatterns": [
                ".stack.yaml",
                "cabal.config",
                "package.yaml"
            ],
            "filetypes": [
                "hs",
                "lhs",
                "haskell"
            ],
            "initializationOptions": {}
        }
    },
    "tslint.jsEnable": true,
    "tslint.enable": true,
    "tsserver.npm": "/usr/local/bin/npm",
    "tsserver.log": "verbose",
    "git.enableGutters": false,
    "git.addGlameToBufferVar": true,
    "python.linting.mypyEnabled": true,
    "python.jediEnabled": true,
    "python.formatting.provider": "yapf",
    "coc.preferences.jumpCommand": "drop",
    "coc.preferences.codeLens.enable": true,
    "codeLens.enable": true,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualTextLines": 4,
    "suggest.noselect": true,
    "suggest.enablePreview": true,
    "list.source.grep.command": "ag",
    "list.source.files.defaultOptions": [
        "--ignore-case"
    ],
    "list.source.grep.defaultOptions": [
        "--ignore-case",
        "--interactive",
        "--auto-preview"
    ],
    "list.source.helptags.defaultOptions": [
        "--ignore-case",
        "--auto-preview"
    ],
    "list.source.words.defaultOptions": [
        "--ignore-case",
        "--interactive"
    ],
    "list.source.buffers.defaultOptions": [
        "--normal"
    ],
    "list.source.commits.defaultOptions": [
        "--normal"
    ],
    "list.source.yank.defaultOptions": [
        "--normal"
    ],
    "list.source.sessions.defaultOptions": [
        "--normal"
    ],
    "suggest.preferCompleteThanJumpPlaceholder": true,
    "git.addedSign.text": "\u2503",
    "git.changedSign.text": "\u2503",
    "git.removedSign.text": "\u2503",
    "git.changeRemovedSign.text": "\u2503",
    "git.topRemovedSign.text": "\u2503",
    "tabnine.priority": 50,
    "yank.priority": 40
}
