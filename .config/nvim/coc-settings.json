{
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {}
    },
    "python":{
        "command": "pyls",
        "filetypes": [
            "py",
            "python"
        ],
        "initializationOptions": {}
    }
  }
}
