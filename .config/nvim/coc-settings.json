{
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {}
    }
  },
  "tslint.jsEnable": true,
  "tslint.enable": true,
  "tsserver.npm": "/usr/local/bin/npm",
  "tsserver.log": "verbose"


}
